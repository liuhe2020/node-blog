<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head') %>
  <body class="d-flex flex-column min-vh-100">
    <%- include('partials/nav') %> <% if( blogs.length > 0) {%> <%
    blogs.forEach(blog=>{%>
    <a href="/blogs/<%= blog.id %>" class="text-decoration-none text-reset">
      <div class="mx-4">
        <h2><%= blog.title %></h2>
        <p><%= blog.body %></p>
      </div>
    </a>
    <% })%> <% } %> <%- include('./partials/footer.ejs') %>
  </body>
</html>
